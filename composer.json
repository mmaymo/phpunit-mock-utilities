{
  "name": "widoz/phpunit-mock-utilities",
  "description": "A set of phpunit mock utilities to make mocking more simpler",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Guido Scialfa",
      "email": "dev@guidoscialfa.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.3",
    "fzaninotto/faker": "^1.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "squizlabs/php_codesniffer": "^3.0",
    "phpcompatibility/php-compatibility": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "Widoz\\PhpUnit\\Mock\\Utilities\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Widoz\\PhpUnit\\Mock\\Utilities\\Tests\\": "tests/php"
    }
  },
  "config": {
    "platform": {
      "php": "7.3"
    }
  }
}
